import * from deemon;
import .common.test;

local base = {0,1,2,3, 10,11,12,13};

test.assertSequence(base.filter(e -> e >= 10), {10,11,12,13});
test.assertSequence(base.filter(e -> e < 10), {0,1,2,3});
test.assertSequence(base.filter(e -> false), {});
test.assertSequence(base.filter(e -> true), {0,1,2,3, 10,11,12,13});

/* TODO: These fail because comparing sequences with unbound items doesn't seem
 *       to work properly, even when sequences are compared with themselves! */
//TODO:test.assertSequence(base.ubfilter(e -> e >= 10));
//TODO:test.assertSequence(base.ubfilter(e -> e < 10));
//TODO:test.assertSequence(base.ubfilter(e -> false));
test.assertSequence(base.ubfilter(e -> true), {0,1,2,3, 10,11,12,13});
