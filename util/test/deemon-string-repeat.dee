import .common.test;

test.assertEqualsTyped("x" * 4, "xxxx");
test.assertEqualsTyped("y" * 4, "yyyy");
test.assertEqualsTyped("z" * 4, "zzzz");

test.assertEqualsTyped("x" * 1, "x");
test.assertEqualsTyped("y" * 1, "y");
test.assertEqualsTyped("z" * 1, "z");

test.assertEqualsTyped("x" * 0, "");
test.assertEqualsTyped("y" * 0, "");
test.assertEqualsTyped("z" * 0, "");

test.assertEqualsTyped("x".bytes() * 4, "xxxx".bytes());
test.assertEqualsTyped("y".bytes() * 4, "yyyy".bytes());
test.assertEqualsTyped("z".bytes() * 4, "zzzz".bytes());

test.assertEqualsTyped("x".bytes() * 1, "x".bytes());
test.assertEqualsTyped("y".bytes() * 1, "y".bytes());
test.assertEqualsTyped("z".bytes() * 1, "z".bytes());

test.assertEqualsTyped("x".bytes() * 0, "".bytes());
test.assertEqualsTyped("y".bytes() * 0, "".bytes());
test.assertEqualsTyped("z".bytes() * 0, "".bytes());

