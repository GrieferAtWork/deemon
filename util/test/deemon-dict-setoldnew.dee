import * from deemon;

local d = Dict();

assert !d.setold("a", 10);
assert d == {};

assert d.setnew("a", 20);
assert d == {"a": 20};

assert !d.setnew("a", 30);
assert d == {"a": 20};

assert d.setold("a", 40);
assert d == {"a": 40};

assert d.setold("a", 50);
assert d == {"a": 50};
