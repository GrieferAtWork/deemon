import deemon;
import doc;
import collections;
import errors;
import * from deemon;

/* Case #1: object originating from within the deemon core */
assert Type.__module__(deemon.Dict) === deemon;

/* Case #2: dex module */
assert Type.__module__(collections.Deque) === collections;

/* Case #3: user-defined ".dee" file */
assert Type.__module__(doc.Doc) === doc;

/* Case #4: Re-export from one module within another (the original module must prevail) */
assert Type.__module__(errors.Error) === deemon;
