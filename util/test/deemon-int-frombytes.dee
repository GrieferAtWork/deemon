import * from deemon;

function assertSigned(expected: int, leBytes: Bytes) {
	assert expected == int.frombytes(leBytes, "little", true);
	assert expected == int.frombytes(leBytes.reversed(), "big", true);
	local nBytes = #leBytes;
	if (nBytes >= 2 && leBytes[nBytes - 2] == leBytes[nBytes - 1])
		assertSigned(expected, leBytes[:-1]);
}

function assertUnsigned(expected: int, leBytes: Bytes) {
	assert expected == int.frombytes(leBytes, "little", false);
	assert expected == int.frombytes(leBytes.reversed(), "big", false);
	local nBytes = #leBytes;
	if (nBytes >= 2 && leBytes[nBytes - 1] == 0x00)
		assertUnsigned(expected, leBytes[:-1]);
}

