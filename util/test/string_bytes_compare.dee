import * from deemon;

local s = "foo";
local b = "bar".bytes();
__asm__("" : "+x" (s));
__asm__("" : "+x" (b));

assert s is string;
assert b is Bytes;

assert (b < s) == true;
assert (s < b) == false;
assert (b <= s) == true;
assert (s <= b) == false;
assert (b == s) == false;
assert (s == b) == false;
assert (b != s) == true;
assert (s != b) == true;
assert (b > s) == false;
assert (s > b) == true;
assert (b >= s) == false;
assert (s >= b) == true;
