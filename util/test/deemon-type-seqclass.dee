import * from deemon;
import collections;

/* Assert that the abstract bases all have the correct sequence class */
assert Type.__seqclass__(Sequence) === Sequence;
assert Type.__seqclass__(Set) === Set;
assert Type.__seqclass__(Mapping) === Mapping;

/* Assert that built-in types all have the correct sequence class */
assert Type.__seqclass__(List) === Sequence;
assert Type.__seqclass__(Tuple) === Sequence;
#if __DEEMON_VARIANT__ == "gatw"
assert Type.__seqclass__(NullableTuple from rt) === Sequence;
#endif /* __DEEMON_VARIANT__ == "gatw" */
assert Type.__seqclass__(HashSet) === Set;
assert Type.__seqclass__(HashSet.Frozen) === Set;
assert Type.__seqclass__(Dict) === Mapping;
assert Type.__seqclass__(Dict.Frozen) === Mapping;

/* Assert that types from "collections" all have the correct sequence class */
assert Type.__seqclass__(collections.Deque) === Sequence;
assert Type.__seqclass__(collections.FixedList) === Sequence;
assert Type.__seqclass__(collections.UniqueDict) === Mapping;
assert Type.__seqclass__(collections.UniqueSet) === Set;
assert Type.__seqclass__(collections.RangeMap) === Mapping;
assert Type.__seqclass__(collections.RBTree) === Mapping;
assert Type.__seqclass__(collections.CachedDict) === Mapping;
assert Type.__seqclass__(collections.Bitset) === Set;
assert Type.__seqclass__(collections.BitsetView) === Set;
