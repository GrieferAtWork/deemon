assert ("x" * 4) == "xxxx";
assert ("y" * 4) == "yyyy";
assert ("z" * 4) == "zzzz";

assert ("x" * 1) == "x";
assert ("y" * 1) == "y";
assert ("z" * 1) == "z";

assert ("x" * 0) == "";
assert ("y" * 0) == "";
assert ("z" * 0) == "";

assert ("x".bytes() * 4) == "xxxx".bytes();
assert ("y".bytes() * 4) == "yyyy".bytes();
assert ("z".bytes() * 4) == "zzzz".bytes();

assert ("x".bytes() * 1) == "x".bytes();
assert ("y".bytes() * 1) == "y".bytes();
assert ("z".bytes() * 1) == "z".bytes();

assert ("x".bytes() * 0) == "".bytes();
assert ("y".bytes() * 0) == "".bytes();
assert ("z".bytes() * 0) == "".bytes();

